[flake8]
# Match black line length
max-line-length = 88
# Don't complain about unused imports in init.py
per-file-ignores = __init__.py:F401

# ignore things black controls already
ignore =
    E203, # whitespace before : 
    W503, # line break before binary operator

exclude=
    # autogenerated code
    meadowflow_pb2_grpc.py, meadowflow_pb2.py,
    meadowgrid_pb2_grpc.py, meadowgrid_pb2.py,